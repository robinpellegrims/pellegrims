{
  "name": "goldgetters",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/goldgetters",
  "projectType": "application",
  "tags": ["platform:react", "type:app", "scope:goldgetters"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "target": "prisma-generate",
          "dependencies": true
        }
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/goldgetters",
        "outputPath": "dist/apps/goldgetters"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/goldgetters"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "goldgetters:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "goldgetters:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "goldgetters:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "goldgetters:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/goldgetters"],
      "options": {
        "jestConfig": "apps/goldgetters/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/goldgetters/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "release": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "postTargets": ["goldgetters:github", "goldgetters:deploy:production"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    },
    "deploy": {
      "executor": "@pellegrims/nx-vercel:deploy",
      "options": {
        "projectId": "prj_ubfBjPKtdCGZKFL8M8B0FS9eTo4U",
        "orgId": "GZ8fDLDaLMVY3YiLUZ86vnk4",
        "baseBranch": "master"
      },
      "configurations": {
        "production": {
          "environment": "production"
        }
      }
    }
  }
}
