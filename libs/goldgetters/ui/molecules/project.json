{
  "name": "goldgetters-ui-molecules",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/goldgetters/ui/molecules/src",
  "projectType": "library",
  "tags": [
    "platform:react",
    "type:ui",
    "atomic-design:molecules",
    "scope:goldgetters"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/goldgetters/ui/molecules/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/goldgetters/ui/molecules"],
      "options": {
        "jestConfig": "libs/goldgetters/ui/molecules/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
